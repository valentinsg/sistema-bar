{
  "functions": {
    "app/api/**/route.ts": {
      "maxDuration": 3,
      "memory": 128
    },
    "app/api/contador/sse/route.ts": {
      "maxDuration": 3,
      "memory": 128
    },
    "app/api/health/route.ts": {
      "maxDuration": 2,
      "memory": 128
    },
    "app/api/debug/env/route.ts": {
      "maxDuration": 2,
      "memory": 128
    }
  },
  "build": {
    "env": {
      "VERCEL_ENV": "@vercel_env"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Connection",
          "value": "close"
        },
        {
          "key": "Keep-Alive",
          "value": "timeout=0"
        }
      ]
    }
  ]
}
